<?xml version="1.0" encoding="UTF-8"?>
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="net.groboclown.idea.p4ic.ui.config.props.ClientNameConfigPartPanel">
  <grid id="27dc6" binding="rootPanel" layout-manager="BorderLayout" hgap="4" vgap="0">
    <constraints>
      <xy x="20" y="20" width="500" height="26"/>
    </constraints>
    <properties/>
    <border type="none"/>
    <children>
      <component id="4e1d8" class="javax.swing.JLabel" binding="listLabel">
        <constraints border-constraint="West"/>
        <properties>
          <labelFor value="4711c"/>
          <text resource-bundle="net/groboclown/idea/p4ic/P4Bundle" key="configuration.clientname"/>
          <verticalAlignment value="0"/>
        </properties>
      </component>
      <grid id="b90d3" binding="listPanel" layout-manager="BorderLayout" hgap="0" vgap="0">
        <constraints border-constraint="Center"/>
        <properties/>
        <border type="none"/>
        <children>
          <component id="4711c" class="javax.swing.JComboBox" binding="clientDropdownList">
            <constraints border-constraint="Center"/>
            <properties>
              <editable value="true"/>
              <toolTipText resource-bundle="net/groboclown/idea/p4ic/P4Bundle" key="configuration.clientname.dropdown-list.tooltip"/>
            </properties>
          </component>
          <grid id="b50e7" binding="refreshPanel" layout-manager="BorderLayout" hgap="0" vgap="0">
            <constraints border-constraint="East"/>
            <properties/>
            <border type="none"/>
            <children>
              <component id="ac846" class="javax.swing.JButton" binding="listRefreshButton">
                <constraints border-constraint="Center"/>
                <properties>
                  <text value=""/>
                  <toolTipText resource-bundle="net/groboclown/idea/p4ic/P4Bundle" key="configuration.refresh-client-list"/>
                </properties>
              </component>
              <component id="973d3" class="com.intellij.util.ui.AsyncProcessIcon" binding="listRefreshSpinner" custom-create="true">
                <constraints border-constraint="West"/>
                <properties/>
              </component>
            </children>
          </grid>
        </children>
      </grid>
    </children>
  </grid>
</form>
